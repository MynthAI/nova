{
  "compilerOptions": {
    "baseUrl": "./",
    "target": "esnext",
    "module": "nodenext",
    "declaration": true,
    "declarationMap": true,
    "emitDeclarationOnly": false,
    "moduleResolution": "nodenext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "dist",
    "lib": ["es2022", "ESNext.Iterator"]
  },
  "include": ["**/*"],
  "exclude": ["node_modules", "dist"]
}
